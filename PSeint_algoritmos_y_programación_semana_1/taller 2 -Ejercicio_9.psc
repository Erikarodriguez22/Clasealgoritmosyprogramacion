Proceso Ejercicio_Salarioneto
	Escribir "Ingrese el numero de horas laboradas"
	Leer horast
	Escribir "Ingrese el valor por hora laborada "
	leer precio
	sbrut<-(horast*precio)
	desc<-(sbrut*0.2)
	stotal<-(sbrut-desc)
	Escribir"El salario neto del trabajador es: ",stotal
	Escribir "El valor descontado es: ", desc
FinProceso