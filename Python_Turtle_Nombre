import turtle 


tortuga=turtle.Turtle()
tortuga.pensize(15)
tortuga.color("purple")
tortuga.shape('turtle')
tortuga.left(180)
tortuga.forward(100)
tortuga.left(90)
tortuga.forward(60)
tortuga.left(90)
tortuga.forward(80)
tortuga.left(180)
tortuga.forward(80)
tortuga.left(90)
tortuga.forward(60)
tortuga.left(90)
tortuga.forward(100)

