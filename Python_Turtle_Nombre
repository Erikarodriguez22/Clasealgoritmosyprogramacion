tortuga=turtle.Turtle()
tortuga.shape('turtle')
tortuga.pensize(15)
tortuga.color("green")
tortuga.penup()
tortuga.goto((105, 0))
tortuga.pendown()
tortuga.forward(120)
tortuga.right(180)
tortuga.forward(60)
tortuga.left(90)
tortuga.forward(120)
tortuga.right(90)
tortuga.forward(60)
