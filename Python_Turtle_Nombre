tortuga=turtle.Turtle()
tortuga.shape('turtle')
tortuga.pensize(15)
tortuga.color("blue")
tortuga.penup()
tortuga.goto((335, -120))
tortuga.pendown()
tortuga.left(70)
tortuga.forward(122)
tortuga.right(140)
tortuga.forward(122)
tortuga.left(180)
tortuga.forward(55)
tortuga.left(70)
tortuga.forward(50)
tortuga.left(180)
tortuga.forward(50)
tortuga.right(70)
tortuga.forward(55)
tortuga.left(70)


import turtle
t=turtle.Turtle()
t.speed(0)
t.pencolor("pink")
t.pensize(5)
for x in range (6):
    t.rt(60)
    for y in range (9):
        t.fd(10)
        t.rt(10)
    t.rt(90)
    for z in range (9):
        t.fd(10)
        t.rt(10)
    t.rt(90)
    
    


t=turtle.Turtle()
t.speed(0)
t.penup()
t.goto((335, -120))
t.pendown()
t.pencolor("pink")
t.pensize(5)



for x in range (6):
    t.rt(60)
    for y in range (9):
        t.fd(10)
        t.rt(10)
    t.rt(90)
    for z in range (9):
        t.fd(10)
        t.rt(10)
    t.rt(90)
